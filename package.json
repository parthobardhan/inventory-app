{
  "name": "textile-inventory-app",
  "version": "1.0.0",
  "description": "A web-based inventory management system for textile products",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required for this PWA'",
    "vercel-build": "echo 'Build complete - PWA ready for deployment'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:ai": "node test-ai-service.js",
    "test:ai:openai": "node test-openai-local.js",
    "test:ai:force": "node -e \"require('./test-ai-service.js').testAIService(true)\"",
    "test:ai:cli": "node test-ai-cli.js"
  },
  "keywords": [
    "inventory",
    "textile",
    "mongodb",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/lib-storage": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "openai": "^4.104.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jsdom": "^27.0.0",
    "nodemon": "^3.0.1",
    "ollama": "^0.5.18",
    "supertest": "^6.3.3"
  }
}
